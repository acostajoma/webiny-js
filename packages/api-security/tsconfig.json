{
  "extends": "../../tsconfig.json",
  "include": ["src", "__tests__/**/*.ts"],
  "references": [
    { "path": "../api-authentication" },
    { "path": "../api-tenancy" },
    { "path": "../error" },
    { "path": "../api" },
    { "path": "../handler-fastify-aws" },
    { "path": "../handler-graphql" },
    { "path": "../fastify" },
    { "path": "../plugins" },
    { "path": "../pubsub" },
    { "path": "../validation" },
    { "path": "../api-tenancy-so-ddb" },
    { "path": "../api-wcp" }
  ],
  "compilerOptions": {
    "rootDirs": ["./src", "./__tests__"],
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-authentication/*": ["../api-authentication/src/*"],
      "@webiny/api-authentication": ["../api-authentication/src"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"],
      "@webiny/api/*": ["../api/src/*"],
      "@webiny/api": ["../api/src"],
      "@webiny/handler-fastify-aws/*": ["../handler-fastify-aws/src/*"],
      "@webiny/handler-fastify-aws": ["../handler-fastify-aws/src"],
      "@webiny/handler-graphql/*": ["../handler-graphql/src/*"],
      "@webiny/handler-graphql": ["../handler-graphql/src"],
      "@webiny/fastify/*": ["../fastify/src/*"],
      "@webiny/fastify": ["../fastify/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"],
      "@webiny/pubsub/*": ["../pubsub/src/*"],
      "@webiny/pubsub": ["../pubsub/src"],
      "@webiny/validation/*": ["../validation/src/*"],
      "@webiny/validation": ["../validation/src"],
      "@webiny/api-tenancy-so-ddb/*": ["../api-tenancy-so-ddb/src/*"],
      "@webiny/api-tenancy-so-ddb": ["../api-tenancy-so-ddb/src"],
      "@webiny/api-wcp/*": ["../api-wcp/src/*"],
      "@webiny/api-wcp": ["../api-wcp/src"]
    },
    "baseUrl": "."
  }
}
